
@article{maturana2019layer,
  title = {Layer Selection in Progressive Transmission of Motion-Compensated JPEG2000 Video},
  author = {Maturana-Espinosa, Jos{\'e} Carmelo and Garc{\'i}a-Ortiz, Juan Pablo and M{\"u}ller, Daniel and Gonz{\'a}lez-Ruiz, Vicente},
  journal = {Electronics},
  volume = {8},
  number = {9},
  pages = {1032},
  year = {2019},
  publisher = {Multidisciplinary Digital Publishing Institute},
  URL = {https://doi.org/10.3390/electronics8091032},
  IMPACT = {(JCR 2018) 1.764},
  RANKINGS = {(JCR 2018) ENGINEERING, ELECTRICAL AND ELECTRONIC: 154/266 (Q3)},
  ISSN = {2079-9292},
  resumen = {Esta publicación muestra las mejoras introducidas en el algoritmo OPTL (Optimized Progressive Transmission by Layers), presentado junto con FSO (Full Search Optimization) anteriormente en el DCC'18 (Data Compression Conference 2018), ambos diseñados para definir la secuencia de paquetes JPEG2000 a transmitir en entornos con ancho de banda limitado, cuando MCJ2K (Motion Compensated JPEG2000) se ha usado para comprimir las secuencias de imágenes que son servidas por un servidor JPIP (JPEG2000 Interactive Protocol). En concreto, ESLA (Estimated-Slope sub-band Layers Allocation), mejora a OPTL en que ahora ya no es necesario haber usado las ganancias de las subbandas temporales para aumentar su rendimiento rate/distortion. Esto además mejora los tiempos de compresión, así como los ratios de compresión porque se evita la generación de paquetes vacíos en el code-stream, como consecuencia del uso de incrementos de calidad muy pequeños al comienzo de la transmisión. Los resultados experimentales demuestran que ESLA arroja un rendimiento desde el punto de vista rate/distortion muy próximo a FSO (renombrado en este trabajo como OSLA (Optimized Sub-band Layers Allocation).}
}

@InProceedings{SH19,
  AUTHOR = 	 {S\'anchez-Hern\'andez, J.J. and
                 Gonz\'alez-Ruiz, V. and
		 Garc{\'i}a-Ortiz, J.P. and
		 M\"uller, D.},
  TITLE = 	 {Client-driven Transmission of JPEG2000 Image Sequences using Motion Compensated Conditional Replenishment},
  BOOKTITLE =    {Proceedings of Data Compression Conference (DCC)},
  YEAR = 	 {2019},
  editor = 	 {Bilgin, A. and
                  Marcellin, M.W. and
		  Serra-Sagrista, J. and
		  Storer, J.A.},
  PAGES = 	 {602--602},
  MONTH = 	 {march},
  address = 	 {Snowbird, Utah, USA},
  organization = {U. Arizona, Brandeis U., Microsoft Research, IEEE Signal Processing Society},
  publisher =    {IEEE Computer Society},
  URL =          {https://doi.org/10.1109/DCC.2019.00114},
  ranking =      {\href{http://portal.core.edu.au/conf-ranks/?search=data+compression+conference&by=all&source=CORE2018&sort=atitle&page=1}{CORE(2018)}: A*},
  ISBN         = {978-1-7281-0657-1},
  ISSN         = {2375-0359},
  resumen      = {En este trabajo se presenta y evalúa MCCR (Motion Compensated Conditional Replenishment), un cliente JPIP (JPEG2000 Interactive Protocol) capaz de mejorar las reconstrucciones de las secuencias transmitidas gracias al uso de estimación de movimiento en el lado del cliente (el servidor y sus datos Motion JPEG2000 permanecen inalterables). Un cliente MCCR genera una predicción para cada imagen que se va a solicitar y recibe del servidor una versión de baja resolución de dicha imagen. Ambas imágenes se comparan (la predicción y la predicha) y el cliente, dependiendo del ancho de banda disponible, descarga un número variable de precintos, solicitando primero aquellos que más error de predicción poseen, aprovechando así todos aquellos precintos que se predicen adecuadamente y que no son reclamados al servidor. Los resultados experimentales demuestran mejoras muy significativas en la calidad de las reconstrucciones cuando se comparan con MJ2K (Motion JPEG2000).}
}

@InProceedings{maturana2018rate,
  AUTHOR = 	 {Maturana-Espinosa, J.C. and
                 Gonz\'alez-Ruiz, V. and
		 Garc{\'i}a-Ortiz, J.P. and
		 M\"uller, D.},
  TITLE = 	 {Rate Allocation for Motion Compensated JPEG2000},
  BOOKTITLE =    {Proceedings of Data Compression Conference (DCC)},
  YEAR = 	 {2018},
  editor = 	 {Bilgin, A. and
                  Marcellin, M.W. and
		  Serra-Sagrista, J. and
		  Storer, J.A.},
  PAGES = 	 {70--79},
  MONTH = 	 {march},
  address = 	 {Snowbird, Utah, USA},
  organization = {U. Arizona, Brandeis U., Microsoft Research, IEEE Signal Processing Society},
  publisher    = {IEEE Computer Society},
  URL          = {https://doi.org/10.1109/DCC.2018.00015},
  ranking      = {\href{http://portal.core.edu.au/conf-ranks/?search=data+compression+conference&by=all&source=CORE2018&sort=atitle&page=1}{CORE(2018)}: A*},
  ISBN         = {978-1-7281-0657-1},
  ISSN         = {2375-0359},
  resumen      = {Se presentan y evalúan tres algoritmos de control del bit-rate en tiempo de descompresión (rate allocation) para el códec de vídeo MCJ2K (Motion Compensated JPEG2000), llamados HPTL (Heuristic Progressive Transmission by Layers), OPTL (Optimized Progressive Transmission by Layers) y FSO (Full Search Optimization). MCJ2K puede ser aplicado a los servidores JPIP (JPEG2000 Interactive Protocol) sin modificarlos. Los algoritmos presentados definen tres formas distintas de transmitir progresivamente (tratando de maximizar la calidad) las secuencias de vídeo MCJ2K. También se analiza el impacto de considerar las ganancias de las diferentes subbandas temporales generadas por MCJ2K. Los experimentos demuestran: (1) que MCJ2K permite mejorar significativamente las tasas de compresión respecto de MJ2K (Motion JPEG2000), (2) que tanto HPTL como OPTL (ambos algoritmos con bajo coste computacional comparados con FSO) mejoran ligeramente la calidad de las reconstrucciones cuando dichas ganancias son consideradas, (3) que el rendimiento de FSO no depende de las ganancias (como se esperaba), y (4) que usando OPTL, MCJ2K es más escalable y arroja un rendimiento comparable a SHVC (Scalable High efficiency Video Coding) en términos rate/distortion, incluso usando un estimador de movimiento mucho más simple.}
}

