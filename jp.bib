@ARTICLE(ORTIZ07e,
  AUTHOR = "J.P. Ortiz and V.G. Ruiz and I. Garc{\'i}a", 
  TITLE = {Efficient Virtual Slide Telepathology Systems with JPEG2000},
  JOURNAL = "Scientific Journal on Applied Information Technology (JAPIT)",
  VOLUME = "3",
  PUBLISHER = "Faculty of Electrical Engineering and Computer Science, University of Maribor, Slovenia",
  PAGES = "16-22",
  ISBN = "",
  ISSN = "1683-1373",
  MONTH = "",
  URL = {http://www.hpca.ual.es/~vruiz/papers/ORTIZ07e.pdf},
  quality_long = "\newline\textbf{Indicios de calidad:} JAPIT es una
                  revista electrónica aceptada por el IET (Institution
                  of Engineering and Technology) e indexada en la base
                  de datos INSPEC",
  YEAR = "2007",
  references = "",
  es_abstract = "\newline\textbf{Resumen:} Este trabajo explora las
                  posibilidades del estándar JPEG 2000 para realizar
                  stitching de imágenes médicas, en concreto, en el
                  campo de la telepatología. Stitching es el proceso
                  de generar imágenes a partir de un conjunto de
                  imágenes de menor tamaño, para así crear una imagen
                  de alta resolución como una composición de estas
                  imágenes. Para que en la imagen de alta resolución
                  no sean perceptibles los bordes de las sub-imágenes,
                  éstas deben capturarse con un cierto solapamiento y
                  las regiones comunes se utilizan para crear un
                  promediado. Cuando este proceso se realiza en el
                  dominio de la imagen es necesario descomprimir las
                  imágenes antes de realizar el promediado. Cuando se
                  realiza en el dominio transformado (el dominio
                  nativo de JPEG 2000), el propio banco de filtros de
                  síntesis de la DWT realiza este trabajo. Por tanto,
                  nuestra propuesta reduce los requerimientos de
                  procesamiento y de memoria para realizar dicha
                  tarea"
)

