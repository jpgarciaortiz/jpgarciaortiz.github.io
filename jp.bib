@Article{SH15,
  AUTHOR = 	 {S\'anchez-Hern\'andez, J.J. and
                  Garc{\'i}a-Ortiz, J.P. and
		  Gonz\'alez-Ruiz, Vicente and
		  M\"uller, D.},
  TITLE = 	 {Interactive Streaming of Sequences of High Resolution JPEG2000 Images},
  JOURNAL = 	 {IEEE Transactions on Multimedia},
  YEAR = 	 {2015},
  VOLUME = 	 {17 (issue: 10)},
  NUMBER = 	 {99},
  PAGES = 	 {1829-1838},
  MONTH = 	 {October},
  URL =          {http://dx.doi.org/10.1109/TMM.2015.2470595},
  cites =        {\href{https://scholar.google.com/scholar?oi=bibs&hl=en&cites=2719566845123423087}{(2020/01/21) 7}},
  IMPACT =       {(JCR 2015) 2.303},
  RANKINGS =     {(JCR 2015) COMPUTER SCIENCE, INFORMATION SYSTEMS: 17/139 (Q1), COMPUTER SCIENCE, SOFTWARE ENGINEERING: 5/104 (Q1), TELECOMMUNICATIONS: 9/77 (Q1)},
  ISSN =         {1520-9210},
  resumen =      {En este trabajo se plantea extender el protocolo JPIP (JPEG2000 Interactive Protocol) para mejorar su rendimiento cuando JPIP es empleado para la transmisión de secuencias de imágenes. Dicha extensión consiste en el diseño de un algoritmo de control de flujo de datos denominado "Continue", que permite modificar los parámetros que de forma interactiva pueden cambiar durante la transmisión (ancho de banda, frame-rate, WOI, etc.), sin necesidad de detener el envío de datos. La parte más pesada del algoritmo recae en el lado del cliente, por lo que la propuesta presenta una buena escalabilidad. Los resultados experimentales indican que un incremento consistente de la calidad de las reconstrucciones, especialmente cuando la latencia del enlace es alta.}
}

